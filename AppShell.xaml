<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Manager_for_3_D_Printing.Views"
    x:Class="Manager_for_3_D_Printing.AppShell">

    <TabBar>
        <ShellContent Title="Models" ContentTemplate="{DataTemplate views:ModelBrowserPage}" />
        <ShellContent Title="Queue"  ContentTemplate="{DataTemplate views:PrintQueuePage}" />

        <ShellContent Title="Library" Icon="icon_library.png">
            <views:ModelBrowserPage />
        </ShellContent>
        <ShellContent Title="Queue" Icon="icon_queue.png">
            <views:PrintQueuePage />
        </ShellContent>
    </TabBar>
</Shell>
